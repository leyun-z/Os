<Page
    x:Class="Os.Views.StorageManagementView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Os.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    Style="{StaticResource PageLoadAnimation}"
    mc:Ignorable="d">

    <Grid>

        <Border
            Padding="8"
            Background="White"
            CornerRadius="12">
            <Border.Effect>
                <DropShadowEffect
                    BlurRadius="8"
                    ShadowDepth="0"
                    Color="White" />
            </Border.Effect>
        </Border>

        <Grid Margin="8">
            <TextBlock FontSize="24" Text="存储管理" />
        </Grid>
    </Grid>
</Page>
